<style>
  .key{
    background-color: yellow;
  }
  .positive{
	background-color: #BED854;
}

.negative{
	background-color: #E72155;
}

.neutral{
	background-color: #666666;
}
@import url('https://fonts.googleapis.com/css?family=Lato:400,700,900');

body,h1,h2,ul,li {
  font-family: 'Lato', sans-serif;
  color: #313131;
}
</style>


<h1> <%= title %> </h1>

<% raw = JSON.parse(data) %>
<% keywords = JSON.parse(sharedWords) %>

  <h2>First Article</h2>
  <ul>
    <% for (i in raw[0].keywords) { %>
      <li><%= raw[0].keywords[i].text.toLowerCase() %> | <strong>relevance</strong>: <%= raw[0].keywords[i].relevance %> | <%= raw[0].keywords[i].sentiment.type %></li>
    <% } %>
  </ul>


<h2>Second Article</h2>
<ul>
  <% for (i in raw[1].keywords) { %>
    <li><%= raw[1].keywords[i].text.toLowerCase() %> | <strong>relevance</strong>: <%= raw[1].keywords[i].relevance %> | <%= raw[1].keywords[i].sentiment.type %></li>
  <% } %>
</ul>

<h2>Shared Keywords</h2>
  <ul>
    <% for (i in keywords) { %>
      <li><%= keywords[i].text.toLowerCase() %> | <strong>relevance</strong>: <%= keywords[i].relevance %> | <%= keywords[i].sentiment.type %></li>
    <% } %>
  </ul>

<%- highlighted %>


